function calculateGrade() {
    const marks = prompt("Enter student's marks (between 0 and 100):");
    if (marks >= 80) {
        console.log("A");
    }else if (marks >= 60) {
        console.log("B");
    }else if (marks >= 50) {
        console.log("C");
    }else if (marks >= 40) {
        console.log("D");
    }else{
        console.log("E");
    }
}

